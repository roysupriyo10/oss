{
  "name": "@roysupriyo10/utils",
  "description": "utils package with common helpers, this package is platform independent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
